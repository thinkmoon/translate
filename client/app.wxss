/**app.wxss**/
@import './assets/css/weui/weui.wxss';
@import './assets/css/font.wxss';



.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  color:#a2c5c9;
} 
.page__hd {
    padding: 40px;
}
.page__bd {
    padding-bottom: 40px;
}
.page__bd_spacing {
    padding-left: 15px;
    padding-right: 15px;
}

.page__ft{
    padding-bottom: 10px;
    text-align: center;
}

.page__title {
    text-align: left;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: left;
    font-size: 14px;
}
page{
    background-color: #EEEEEE;
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.gray{
  color:#d6cfc9;
}
.cyan{
  color: #edf8fc;
}
.blue{
  color: #a2c5c9;
}
.black{
  color: #2b393c;
}

.btn{
  background-color: #a2c5c9;
}





